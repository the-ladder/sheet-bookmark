{
    "manifest_version": 2,
    "name": "sheet-bookmark",
    "short_name": "s-bm",
    "author": "unkownY",
    "version": "1.0",
    "description": "a simple bookmark searcher & trimmer",
    "icons": {
        "48": "icons/border-48.png"
    },
    "permissions": [
        "bookmarks",
        "tabs"
    ],
    "browser_action": {
        "browser_style": true,
        "default_icon": "icons/beasts-48.png",
        "default_title": "sheet !",
        "default_popup": "popup/search-bookmark.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}